  "dependencies": {
    "axios": "1.1.3",
    "camelcase": "^7.0.1",
    "humps": "^2.0.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.40",
    "pinia": "^2.1.3",
    "pubsub-js": "^1.9.4",
    "qs": "^6.11.0",
    "register-service-worker": "^1.7.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2",
    "vue3-lottie": "^3.2.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
